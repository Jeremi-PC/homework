select * from employees where job_id like 'FI_ACCOUNT';
select * from employees where salary < 12000;
select * from employees where department_id in (70, 80, 100);
select * from employees where first_name like 'D%';
select * from employees where employee_id between 108 and 143;
select first_name, last_name from employees where phone like '590.%';
select * from employees where salary > 1000 and job_id like 'IT_PROG' and phone like '590.%';
select first_name, last_name, salary from employees where salary between 8000 and 10000 and commission_pct not null;
